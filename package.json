{
  "name": "coffee_billboard",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "5.4.0",
    "@material-ui/core": "4.4.1",
    "@material-ui/icons": "4.4.1",
    "@material-ui/lab": "4.0.0-alpha.27",
    "@types/body-parser": "1.17.1",
    "@types/classnames": "2.2.9",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.1",
    "@types/memoizee": "0.4.3",
    "@types/node": "12.7.3",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-infinite-scroll-component": "^4.2.5",
    "@types/react-redux": "7.1.2",
    "@types/react-router-dom": "4.3.5",
    "@types/redux-actions": "2.6.1",
    "@types/shelljs": "0.8.5",
    "axios": "0.19.0",
    "classnames": "2.2.6",
    "concurrently": "4.1.2",
    "connected-react-router": "6.5.2",
    "dotenv": "8.1.0",
    "express-validator": "6.2.0",
    "final-form": "4.18.5",
    "memoizee": "0.4.14",
    "multer": "^1.4.2",
    "mysql": "2.17.1",
    "node-sass": "4.12.0",
    "nodemon": "1.19.1",
    "notistack": "0.9.0",
    "query-string": "^6.8.3",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-final-form": "6.3.0",
    "react-infinite-scroll-component": "4.5.3",
    "react-redux": "7.1.1",
    "react-router": "5.0.1",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.1.2",
    "redux": "4.0.4",
    "redux-actions": "2.6.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0",
    "rimraf": "3.0.0",
    "shelljs": "0.8.3",
    "stylelint": "10.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.19.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-import-group-ordering": "2.1.2",
    "tslint-no-circular-imports": "0.7.0",
    "tslint-react": "4.0.0",
    "typed-scss-modules": "0.0.12",
    "typeorm": "0.2.19",
    "typescript": "3.5.2"
  },
  "scripts": {
    "start": "concurrently \"npm run start:api\" \"npm run scss:watch\" \"npm run start:app\"",
    "start:api": "nodemon --watch \"api/\" --exec \"ts-node -r tsconfig-paths/register -P api/tsconfig.json api/server.ts\" -e ts",
    "start:app": "nodemon --watch src/app/theme/theme-overrides.less --ext less --legacy-watch --verbose --exec \"craco start\"",
    "build": "npm run clean && npm run build:api && npm run build:app && npm run move-app-to-api",
    "build:app": "npm run scss:generate && craco build",
    "build:api": "tsc -p api && npm run copy-api-ts-config",
    "serve": "cd dist/api && node -r tsconfig-paths/register server.js",
    "test": "npm run scss:generate && craco test --transformIgnorePatterns \"node_modules/(?!(antd))/\"",
    "tslint": "mkdirp reports && tslint --config tslint.json --project tsconfig.json --format prose --out reports/tslint-report.txt",
    "stylelint": "mkdirp reports && stylelint 'src/**/*.scss' --syntax scss > reports/stylelint-report.txt",
    "scss:watch": "npm run scss:generate -- --watch",
    "scss:generate": "tsm \"src/app/**/*.module.scss\" --nameFormat camel --includePaths src/app --exportType default --aliasPrefixes.~app src/app",
    "copy-api-ts-config": "node util/copy-api-ts-config.js",
    "move-app-to-api": "node util/move-app-to-api.js",
    "typeorm": "ts-node -r tsconfig-paths/register -P api/tsconfig.json ./node_modules/typeorm/cli.js",
    "clean": "rimraf build dist",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
